# development configuration

server.port=8009

# logging
logging.level.root=WARN
logging.level.com.atto.server=TRACE

#JWT
jwt.token.cookie=AUTH-TOKEN-X
jwt.token.header=Authorization
jwt.token.secret=atto
# seconds
jwt.token.expires.interval=600
# days
jwt.password.expires.interval=90

# web socket
ws.url=/atto/ws

#Database
mybatis.config-location=classpath:mybatis-config.xml

# mysql
spring.datasource.schema=classpath:import.sql
spring.datasource.driverClassName=com.mysql.jdbc.Driver
spring.datasource.url=jdbc:mysql://localhost:3306/atto?verifyServerCertificate=false&useSSL=false&requireSSL=false
spring.datasource.username=attodev
spring.datasource.password=atto1234

